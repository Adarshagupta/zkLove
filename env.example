# Blockchain Configuration
PRIVATE_KEY=your_private_key_here
MAINNET_RPC_URL=https://mainnet.infura.io/v3/your_infura_key
GOERLI_RPC_URL=https://goerli.infura.io/v3/your_infura_key
SEPOLIA_RPC_URL=https://sepolia.infura.io/v3/your_infura_key
ETHERSCAN_API_KEY=your_etherscan_api_key

# Contract Addresses (will be populated after deployment)
IDENTITY_VERIFICATION_CONTRACT=
VERIFIER_CONTRACT=

# API Keys for services
INFURA_PROJECT_ID=your_infura_project_id
ALCHEMY_API_KEY=your_alchemy_api_key

# Mopro Configuration
MOPRO_CIRCUIT_PATH=assets/circuits/identity_verification.r1cs
MOPRO_PROVING_KEY_PATH=assets/circuits/identity_verification.zkey
MOPRO_VERIFICATION_KEY_PATH=assets/circuits/verification_key.json

# App Configuration
APP_ENV=development
DEBUG_MODE=true

# Face Recognition API (if using external service)
FACE_API_KEY=your_face_api_key
FACE_API_ENDPOINT=https://api.face-recognition-service.com

# ID Verification API (if using external service)
ID_API_KEY=your_id_verification_api_key
ID_API_ENDPOINT=https://api.id-verification-service.com

# IPFS Configuration (for storing metadata)
IPFS_PROJECT_ID=your_ipfs_project_id
IPFS_PROJECT_SECRET=your_ipfs_project_secret
IPFS_ENDPOINT=https://ipfs.infura.io:5001

# Database Configuration (if using backend)
DATABASE_URL=your_database_url
REDIS_URL=your_redis_url

# Security
JWT_SECRET=your_jwt_secret
ENCRYPTION_KEY=your_encryption_key

# Analytics
ANALYTICS_API_KEY=your_analytics_api_key

# Push Notifications
FCM_SERVER_KEY=your_fcm_server_key
APNS_KEY_ID=your_apns_key_id
APNS_TEAM_ID=your_apns_team_id
